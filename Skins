            <select id="selectbox-bbs-history">
                <option value="" selected=""><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Recently visited bulletin board</font></font></option>
            <option value="https://www.inven.co.kr/board/lostark/4811"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Lost arc sleep</font></font></option><option value="https://www.inven.co.kr/board/lostark/4823"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Lost Ark Screenshots</font></font></option></select>
